    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5",
    "vitest": "^2.1.1"
  }
