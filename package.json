    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.16.7",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-data-grid": "^7.19.0",
    "@mui/x-date-pickers": "^7.2.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.62.0",
    "axios": "^1.7.4",
    "date-fns": "^3.6.0",
    "formik": "^2.4.5",
    "jwt-decode": "^4.0.0",
    "next": "14.2.12",
    "react": "^18",
    "react-dom": "^18",
