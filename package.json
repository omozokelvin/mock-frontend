{
  "name": "aryon-frontend-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prelint": "tsc --noemit",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
